{
  "name": "jestframework",
  "version": "1.0.0",
  "description": "Java script testing framework",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "watch:test": "jest --watch"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/*.js","src/**/*.js"
    ],
    "coverageReporters": ["json", "lcov", "text", "html", "clover"],
    "reporters": [
      "default",
      ["jest-html-reporters", {
        "publicPath": "./jest-html-report",
        "filename": "JestAutomationTestReport.html",
        "logoImgPath": "./report-logo/logo.png",
        "expand": true,
        "openReport": true
      }]
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 50,
        "lines": 33,
        "statements": 25
      }
    }
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arunrajput1987/jestFramework.git"
  },
  "author": "Arun Kumar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/arunrajput1987/jestFramework/issues"
  },
  "homepage": "https://github.com/arunrajput1987/jestFramework#readme",
"devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "handlebars": "^4.7.7",
    "jest": "^27.0.6",
    "jest-cli": "^27.0.6",
    "jest-html-reporters": "^2.1.6",
    "react": "^17.0.2",
    "react-test-renderer": "^17.0.2"
  }
}
